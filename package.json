  "devDependencies": {
    "@next/eslint-plugin-next": "14.2.11",
    "@svgr/webpack": "8.1.0",
    "@types/canvas-confetti": "1.6.4",
    "@types/node": "20.16.5",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/sanitize-html": "2.13.0",
    "@types/tryghost__content-api": "1.3.17",
