    "axios": "1.7.7",
    "build-route-tree": "1.0.3",
    "canvas-confetti": "1.9.3",
    "classnames": "2.5.1",
    "connectkit": "1.8.2",
    "dayjs": "1.11.13",
    "final-form": "4.20.10",
    "html-react-parser": "5.1.16",
    "jwt-decode": "4.0.0",
    "next": "14.2.11",
    "next-auth": "5.0.0-beta.20",
    "react": "18.3.1",
    "react-countup": "6.5.3",
    "react-dom": "18.3.1",
    "react-final-form": "6.5.9",
    "react-markdown": "9.0.1",
    "react-redux": "9.1.2",
    "react-remove-scroll": "2.6.0",
    "react-responsive": "10.0.0",
